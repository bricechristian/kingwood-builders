---

const { data } = Astro.props;
---

<section class="py-40">
    <div class="grid-layout text-center">
        <div class="grid-span-xl">
            <h1
                class="justify-center"
                data-viewport="typography-shift">
                <span>{data.title}</span>
            </h1>
        </div>
    </div>
</section>